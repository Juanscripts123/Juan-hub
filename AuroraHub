
local function enviarNotificacao(mensagem)
    game.StarterGui:SetCore("SendNotification", {
        Title = "A-U-R-O-R-A Hub v2",
        Text = mensagem,
        Duration = 5
    })
end

enviarNotificacao("bem vindo(a) ao A-u-r-o-r-a Hub v2!, espero que se divirta ")

