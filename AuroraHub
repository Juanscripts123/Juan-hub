label.Parent = quadro
label.BackgroundColor3 = preto
label.Size = UDim2.new(1, 0, 1, 0)
label.Position = UDim2.new(0, 0, 0, 0)
label.Text = "" -- Define o texto como vazio para que n√£o apare√ßa nada

-- Espera 2 segundos
task.wait(2)

-- Remove o label
label:Destroy()

-- Remove o ScreenGui (opcional, se quiser remover o ScreenGui tamb√©m)
-- quadro:Destroy()
end
end
