        wait(0.1) -- Espera um pouco antes de mover novamente
    end
end

-- Conecta a fun√ß√£o de movimento
movePart()

-- Fun√ß√£o para lidar com a colis√£o
local function onTouched(hit)
    print("A parte tocou em: " .. hit.Name) -- Imprime o nome do objeto tocado
    part:Destroy() -- Destr√≥i a parte ap√≥s a colis√£o
end

-- Conecta a fun√ß√£o onTouched ao evento Touched da parte
